{
  "name": "rx-solid",
  "version": "0.0.9",
  "description": "effect-rx for solid",
  "license": "MIT",
  "author": "frank-iii",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frank-iii/rx-solid.git"
  },
  "homepage": "https://github.com/frank-iii/rx-solid#readme",
  "bugs": {
    "url": "https://github.com/frank-iii/rx-solid/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@effect/platform": "^0.80.15",
    "@effect/platform-browser": "^0.60.6",
    "@types/node": "^20.17.32",
    "@types/scheduler": "^0.23.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.5",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "jsdom": "^24.1.3",
    "prettier": "3.0.0",
    "solid-js": "^1.9.5",
    "tsup": "^8.4.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.18",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@effect-rx/rx": "^0.38.6",
    "effect": "^3.14.15",
    "scheduler": "^0.23.2"
  }
}
